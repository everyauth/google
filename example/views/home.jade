- if (!everyauth.loggedIn)
  h2 Not Authenticated
  #google-login
    a.google(href='/auth/google')
      span#google_login_text Login with Google
- else
  h2 Authenticated
  - if (everyauth.google)
    h3 Google User Data
    p= JSON.stringify(everyauth.google.user)
  h3
    a(href='/logout') Logout
